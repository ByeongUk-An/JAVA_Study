package chap04;

public class Continue_quiz {

	public static void main(String[] args) {
		for (int i = 1; i <= 19; i++) {
			for (int j = 1; j <= 9; j++) {
				
				if(i%2!=0) {
					System.out.println(i + "*" + j + "=" + i * j);
				}
						
				
				
			}
			
			if(i%2!=0) {
				System.out.println(i+"단 끝!!");
			}else {
				System.out.println(i+"단 패스");
			}
		}
	}

}
