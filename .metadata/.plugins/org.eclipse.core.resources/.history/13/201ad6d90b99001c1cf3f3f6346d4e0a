package Exex;

import java.util.Scanner;

public class Ex7 {
	public static void main(String[] args) {
		System.out.println("양의 정수 10개를 입력하시오");
		int[] arr = new int[10];
		Scanner sc = new Scanner(System.in);
		for (int i = 0; i < 10; i++) {
			arr[i] = sc.nextInt();

		}
		for (int i = 0; i < arr.length; i++) {
//			System.out.print(arr[i]+" ");
			if(arr[i]%3==0) {
				System.out.println(arr[i]+" ");
			}
		}
	}
}
